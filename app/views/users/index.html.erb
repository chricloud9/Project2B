<h1>Users</h1>

<table>
	<tr> 
		<th> First Name </th>
		<th> Last Name </th>
		<th> Info </th>
	</tr>
     <% @users.each do |user| %>
		<tr>
			<td> <%= user.first_name %> </td>
			<td> <%= user.last_name %> </td>  
			<td> <%= link_to "User Details", user_path(user)%>
		</tr>
    <% end %>
</table>